<script type="text/javascript">
    net.sf.files = {"pyhook": {"staging_days": 3, "legacy_release_notes": null, "mirrors": 0, "downloads": 234199, "exclude_reports": false, "explicitly_staged": false, "files_url": "/projects/pyhook/files/", "link": "", "path": "", "stage": 0, "sha1": "", "name": "pyhook", "default": "", "staged": false, "download_url": "https://sourceforge.net/projects/pyhook/files/pyhook/download", "url": "/projects/pyhook/files/pyhook/", "downloadable": false, "authorized": null, "download_label": "", "md5": "", "type": "d", "full_path": "pyhook"}};
    net.sf.staging_days = 3;
    $(function ($) {
        $('#files').files();
    });
    $(window).load(function() {
        $('.fs-sparklines').each(function(){
          $(this).attr('src', $(this).attr('data-src'));
        });

        var tsConfig = {
            widgets: ['zebra'],
            sortForce: [[4, 0]],
            headers: {
                1: { sorter: 'abbrdate' },
                2: { sorter: 'filesize' },
                3: { sorter: 'localnum' },
                4: { sorter: 'type'}
            },
            textExtraction: function (node) {
                return node.innerHTML;
            }
        };

        if ($('#files_list tbody tr').length) {
            $('#files_list').tablesorter(tsConfig);
            $('#files_list tr th#parent_folder').unbind();
            $('#files_list tr th.header').not('.typesort').not('#parent_folder').addClass('usersortable').append('<span class="ui-icon fs-toggle"><\/span>');
        }
    });
</script>

        
